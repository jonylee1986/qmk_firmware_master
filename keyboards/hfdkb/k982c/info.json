{
    "manufacturer": " ",
    "keyboard_name": "EWEADN-X75",
    "url": "https://github.com/jonylee1986",
    "maintainer": "jonylee@hfd",
    "processor": "WB32FQ95",
    "bootloader": "wb32-dfu",
    "usb": {
        "vid": "0xFFFE"
    },
    "host": {
        "default": {
            "nkro": true
        }
    },
    "features": {
        "bootmagic": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true,
        "encoder": true,
        "dip_switch": true
    },
    "eeprom": {
        "wear_leveling": {
            "driver": "spi_flash",
            "backing_size": 4096
        }
    },
    "dip_switch": {
        "pins": ["B10"]
    },
    "encoder": {
        "enabled": true,
        "rotary": [
            {"pin_a": "C13", "pin_b": "C14"}
        ]
    },
    "matrix_pins": {
        "cols": ["C1", "C2", "C3", "A0", "A1", "A2", "A3", "A4", "A5", "A6", "A7", "C4", "C5", "B0", "B1"],
        "rows": ["B15", "C6", "C7", "C8", "C9", "B14"],
        "custom_lite": true
    },
    "diode_direction": "ROW2COL",
    "rgb_matrix": {
        "driver": "snled27351",
        "animations": {
            "band_spiral_val": true,
            "breathing": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "jellybean_raindrops": true,
            "pixel_rain": true,
            "typing_heatmap": true,
            "digital_rain": true,
            "solid_reactive_simple": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_multinexus": true,
            "splash": true,
            "solid_splash": true
        },
        "sleep": true
    }
}
