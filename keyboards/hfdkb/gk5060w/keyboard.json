{
    "manufacturer": " ",
    "keyboard_name": "metafor formless - wired",
    "maintainer": "jonylee@hfd",
    "url": "https://github.com/jonylee1986/qmk_firmware_master/tree/hfdkb_playground",
    "processor": "WB32FQ95",
    "bootloader": "wb32-dfu",
    "usb": {
        "vid": "0xFFFE",
        "pid": "0x0024",
        "device_version": "1.0.6",
        "suspend_wakeup_delay": 1000
    },
    "host": {
        "default": { "nkro": true }
    },
    "diode_direction": "ROW2COL",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true
    },
    "matrix_pins": {
        "cols": ["C1","C2","C3","A0","A1","A2","A3","A4","A5","A6","A7","C4","C5", "B0"],
        "rows": ["C6", "C7", "C8", "C9", "B14"]
    },
    "dynamic_keymap": {
        "layer_count": 6
    },
    "eeprom": {
        "driver": "wear_leveling",
        "wear_leveling": {
            "driver": "spi_flash",
            "backing_size": 4096
        }
    },
    "ws2812": {
        "pin": "C15"
    },
    "rgb_matrix": {
        "driver": "ws2812",
        "max_brightness": 160,
        "react_on_keyup": true,
        "sleep": true,
        "val_steps": 40,
        "hue_steps": 16,
        "speed_steps": 64,
        "animations": {
            "solid_color": true,
            "breathing": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "jellybean_raindrops": true,
            "typing_heatmap": true,
            "digital_rain": true,
            "solid_reactive_multinexus": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_simple": true,
            "splash": true
        },
        "default": {
            "val": 120,
            "speed": 192
        },
        "layout": [
            { "flags": 2, "x":  25, "y": 16 },
            { "flags": 2, "x":  41, "y": 16 },
            { "flags": 2, "x":  58, "y": 16 },
